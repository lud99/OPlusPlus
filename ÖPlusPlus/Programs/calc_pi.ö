float rand_range_float (int b, float min, int c, float max) => {
    print("%d\n", b);
    print("%f\n", min);
    print("%d\n", c);
    print("%f\n", max);
    return 1.0;//return ((max - min) * (to_float(rand()) / 32767.0)) + min;
};
