float pi() => {
    int inside = 0;
    int its = 1000;

    for (int i = 0, i < its, i++) {
        float x = rand_range_float(-1.0, 1.0);
        float y = rand_range_float(-1.0, 1.0);

        if (x * x + y * y <= 1.0) {
            inside++;
        };
    };

    return 4 * inside / its;
};

//srand(time(0));

print("%i\n", pi());