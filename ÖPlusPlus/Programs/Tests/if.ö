int a;

// a == 10
if (3 > 2) {
	if (2 == 2) {
		a = 10;
	} else {
		a = 3;
	}
} else {
	a = 5;
};

printf("%i\n", a);

a = 0;

printf("%i\n", a);

// a == 3
if (a < 3) {
	if (a == 2) {
		a = 10;
	} else {
		a = 3;
	}
} else {
	a = 5;
};

printf("%i\n", a);

// a == 5
if (a > 3) {
	if (a == 2) {
		a = 10;
	} else {
		a = 3;
	}
} else {
	a = 5;
};

printf("%i\n", a);